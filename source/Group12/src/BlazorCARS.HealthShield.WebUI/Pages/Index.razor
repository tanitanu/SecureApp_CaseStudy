@code{

    private EventCallback LoadComplete{ get; set; }

    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if(firstRender)
        {
            if (LoadComplete.HasDelegate)
                LoadComplete.InvokeAsync();
        }
    }
}
